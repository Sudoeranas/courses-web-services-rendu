<definitions name="ProductsService" targetNamespace="http://localhost:8000/products" xmlns:tns="http://localhost:8000/products" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<types>
		<schema targetNamespace="http://localhost:8000/products">
			<element name="CreateProductRequest">
				<complexType>
					<sequence>
						<element name="name" type="xsd:string" />
						<element name="about" type="xsd:string" />
						<element name="price" type="xsd:decimal" />
					</sequence>
				</complexType>
			</element>
			<element name="CreateProductResponse">
				<complexType>
					<sequence>
						<element name="id" type="xsd:string" />
						<element name="name" type="xsd:string" />
						<element name="about" type="xsd:string" />
						<element name="price" type="xsd:string" />
					</sequence>
				</complexType>
			</element>
			<element name="GetProductsRequest">
				<complexType>
					<sequence>
					</sequence>
				</complexType>
			</element>
			<element name="GetProductsResponse">
				<complexType>
					<sequence>
						<element maxOccurs="unbounded" ref="Product" />
					</sequence>
				</complexType>
			</element>
			<element name="Product">
				<element name="id" type="xsd:string" />
				<element name="name" type="xsd:string" />
				<element name="about" type="xsd:string" />
				<element name="price" type="xsd:string" />
			</element>
		</schema>
	</types>
	<message name="CreateProductRequestMessage">
		<part name="request" element="tns:CreateProductRequest" />
	</message>
	<message name="CreateProductResponseMessage">
		<part name="response" element="tns:CreateProductResponse" />
	</message>
	<message name="GetProductsRequestMessage">
		<part name="request" element="tns:GetProductsRequest" />
	</message>
	<message name="GetProductsResponseMessage">
		<part name="response" element="tns:GetProductsResponse" />
	</message>
	<portType name="ProductsPortType">
		<operation name="CreateProduct">
			<input message="tns:CreateProductRequestMessage" />
			<output message="tns:CreateProductResponseMessage" />
		</operation>
		<operation name="GetProducts">
			<input message="tns:GetProductsRequestMessage" />
			<output message="tns:GetProductsResponseMessage" />
		</operation>
	</portType>
	<binding name="ProductsBinding" type="tns:ProductsPortType">
		<binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http" />
		<operation name="CreateProduct">
			<operation soapAction="http://localhost:8000/products/CreateProduct" />
			<input>
				<body use="encoded" namespace="http://localhost:8000/products" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
			</input>
			<output>
				<body use="encoded" namespace="http://localhost:8000/products" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
			</output>
		</operation>
		<operation name="GetProducts">
			<operation soapAction="http://localhost:8000/products/GetProducts" />
			<input>
				<body use="encoded" namespace="http://localhost:8000/products" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
			</input>
			<output>
				<body use="encoded" namespace="http://localhost:8000/products" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
			</output>
		</operation>
	</binding>
	<service name="ProductsService">
		<port name="ProductsPort" binding="tns:ProductsBinding">
			<address location="http://localhost:8000/products" />
		</port>
	</service>
</definitions>
